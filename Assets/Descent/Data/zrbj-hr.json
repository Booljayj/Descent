					{
						"notIfConditions":null,
						"option":"\"You could have told me yourself.\"",
						"ifConditions":null,
						"linkPath":"toldYouWhatThere"
					},
					{
						"notIfConditions":null,
						"option":"\"I don't know. I just don't know.\"",
						"ifConditions":null,
						"linkPath":"iNeverWantedToHu"
