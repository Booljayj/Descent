			"iDontKnowIDontKn":{
				"content":[
					"\"I don't know. I don't know how anyone can trust anyone. You just have to try, I suppose. You have to want to. Do you want to?\"",
					{
						"notIfConditions":null,
						"option":"\"Yes.\"",
						"ifConditions":null,
						"linkPath":"ohPreciousOkayOk"
					},
					{
						"notIfConditions":null,
						"option":"\"No.\"",
						"ifConditions":null,
						"linkPath":"iSeeISeeWellItSe"
